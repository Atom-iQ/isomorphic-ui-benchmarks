import serverRender from './util/serverRender'
import App from './components/App'

<${input.pageLayout}>
    <@body>
        <div id="searchResultsMount">
            <${function(out) {
                serverRender(App, input.searchResults, out);
            }}/>
        </div>
        <script>
            window.searchResultsData=${JSON.stringify(input.searchResults)};
        </script>
    </@body>
</>
